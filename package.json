{
  "name": "@lencana/lms-platform",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run dev\" \"cd backend && npm run dev\"",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "backend:install": "cd backend && npm install",
    "backend:dev": "cd backend && npm run dev",
    "install:all": "npm install && cd backend && npm install",
    "vercel-build": "npm run build",
    "deploy": "vercel --prod",
    "deploy-preview": "vercel",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:setup": "node scripts/setup-db.js",
    "modules:setup": "node scripts/setup-modules.js",
    "neon:setup": "node scripts/setup-neon-db.js",
    "assets:analyze": "node scripts/optimize-assets.js",
    "migrate": "node database/run-migrations.js",
    "migrate:dev": "NODE_ENV=development npm run migrate",
    "postinstall": "echo 'Installation complete'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "@tailwindcss/vite": "^4.1.7",
    "@vercel/blob": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "framer-motion": "^12.23.12",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "lucide-react": "^0.542.0",
    "pg": "^8.16.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^7.8.2",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "three": "^0.179.1",
    "zod": "^3.24.1",
    "concurrently": "^9.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.179.0",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.15.0",
    "terser": "^5.43.1",
    "vite": "^7.1.3",
    "vitest": "^2.1.8"
  }
}
